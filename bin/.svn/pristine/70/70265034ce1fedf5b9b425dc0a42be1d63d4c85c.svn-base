package com.marolix.Bricks99.utility;

import java.time.LocalDateTime;

public class ErrorInfo {
	private String errorMesssaage;
	private Integer errorCode;
	private LocalDateTime time;

	public String getErrorMesssaage() {
		return errorMesssaage;
	}

	public void setErrorMesssaage(String errorMesssaage) {
		this.errorMesssaage = errorMesssaage;
	}

	public Integer getErrorCode() {
		return errorCode;
	}

	public void setErrorCode(Integer errorCode) {
		this.errorCode = errorCode;
	}

	public LocalDateTime getTime() {
		return time;
	}

	public void setTime(LocalDateTime time) {
		this.time = time;
	}

}
